.wrapper{
    padding-bottom: 160rpx;
}

.head{
    width: 100%;
    height: 200rpx;
    position: relative;
    margin-bottom: 30rpx;
}
.head button{
    width: 112rpx;
    height: 112rpx;
    border-radius: 50%;
    -webkit-border-radius: 50%;
    -moz-border-radius: 50%;
    -ms-border-radius: 50%;
    -o-border-radius: 50%;
    position: absolute;
    left: 50%;
    top: 40%;
    transform: translate(-50%,-50%);
    overflow: hidden;
}
.head image{
    width: 100%;
    height: 100%;
}
.head .nickname{
    font-size: 30rpx;
    color: #353434;
    position: absolute;
    bottom: 10rpx;
    left: 50%;
    transform: translateX(-50%) ;
    -webkit-transform: translateX(-50%) ;
    -moz-transform: translateX(-50%) ;
    -ms-transform: translateX(-50%) ;
    -o-transform: translateX(-50%) ;
}

.my-Legend{
    display: flex;
    justify-content: space-around;
    margin-bottom: 30rpx;
}
.my-Legend .item{
    font-size: 26rpx;
    text-align: center;
    line-height: 2;
}
.my-Legend .item view{
    color: #353434;
}
.my-Legend .item text{
    color: #61B66D;
    font-weight: bold;
}


  .match-list .item .team-member image{
    width: 100%;
    height: 100%;
    
  }

  .title{
      font-size: 32rpx;
      color: #353434;
  }
  .match-list{
      padding: 20rpx;
  }
  .match-list .item{
    position: relative;
    overflow: hidden;
    border-radius: 10rpx;
    background: #fff;
    margin-bottom: 20rpx;
    box-shadow: 0 0 30rpx rgba(203,157,55,0.2);
    padding: 16rpx;
}

  .match-list .item .team-title{
    font-size: 28rpx;
    padding: 10rpx 0;
    position: relative;
    margin-bottom: 10rpx;
    color: #353434;
  }
  

  .match-list .item .team-member{
    width: 92rpx;
    height:92rpx;
    float: left;
    border-radius: 50%;
    overflow: hidden;
    display: flex;
    align-items: center;
    justify-content: center;
    -webkit-border-radius: 50%;
    -moz-border-radius: 50%;
    -ms-border-radius: 50%;
    -o-border-radius: 50%;
    margin-right: 20rpx;
  }


  .match-list .join-match{
      position: absolute;
      color: #61B66D;
      border: 1rpx solid #61B66D;
      border-radius: 100rpx;
      right: 30rpx;
      top: 50%;
      transform: translateY(-50%);
      margin-top: 30rpx;
      width: 144rpx;
      height: 48rpx;
      font-size: 24rpx;
      line-height: 48rpx;
      text-align: center;
}

.launch-match{
    width: 298rpx;
    height: 96rpx;
    background: #61B66D;
    color: #fff;
    display: flex;
    justify-content: center;
    align-items: center;
    border-radius: 100rpx;
    -webkit-border-radius: 100rpx;
    -moz-border-radius: 100rpx;
    -ms-border-radius: 100rpx;
    -o-border-radius: 100rpx;
    line-height: 80rpx;
    font-size: 30rpx;
    position: fixed;
    bottom: 40rpx;
    left: 50%;
    transform: translateX(-50%);
    -webkit-transform: translateX(-50%);
    -moz-transform: translateX(-50%);
    -ms-transform: translateX(-50%);
    -o-transform: translateX(-50%);
    font-size: 32rpx;
}